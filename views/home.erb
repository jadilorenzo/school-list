<h1>Your Subjects</h1>
<a href="/add">Add...</a>

<% @list.each do |item| %>
 <div class='list-wrap'>
    <li>
        <div>
          <p><%= item.title %> - <%= item.time %> minutes <a class="remove" href="/subjects/<%= item.id() %>/remove">remove</a> <a class="edit" href="/subjects/<%= item.id() %>/edit">edit</a></p>
        </div>
    </li>
  </div>
<% end %>
